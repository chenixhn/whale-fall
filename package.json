{
    "name": "whale-fall",
    "version": "1.0.0",
    "description": "A Vue.js project",
    "author": "chenixhn <chenixh@163.com>",
    "private": true,
    "scripts": {
        "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
        "start": "npm run dev",
        "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
        "e2e": "node test/e2e/runner.js",
        "test": "npm run unit && npm run e2e",
        "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
        "eslint": "eslint --ext .js,.vue src",
        "mocklint": "node ./node_modules/mocklint/index.js  `git diff --staged --name-only --diff-filter=ACMRTU -- ./src/mock/*.json`",
        "filenamelint": "node node_modules/validate_filename/index.js -e '^[a-z0-9\\-_./]+$|^src/App.vue$' -m '约定文件名由小写字母、数字、_ 、- 组成' `git diff --staged --name-only --diff-filter=ACMRTU -- ./src/**`",
        "stylelint": "stylelint './src/**/*.scss' './src/**/*.vue' --config ./.stylelintrc",
        "precommit": "npm run stylelint && npm run eslint && npm run mocklint && npm run filenamelint",
        "build": "node build/build.js",
        "eslintfix": "node_modules/.bin/eslint --fix './src/**/*.vue'"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "babel-polyfill": "^6.26.0",
        "body-parser": "^1.18.3",
        "echarts": "^4.1.0",
